{
  "name": "swaz-solutions",
  "private": true,
  "version": "1.1.1",
  "type": "module",
  "scripts": {
    "dev:frontend": "vite",
    "dev:backend": "nodemon backend/server.js",
    "dev": "concurrently \"npm:dev:frontend\" \"npm:dev:backend\"",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node backend/server.js",
    "deploy": "npm run build && npm start",
    "test": "npx jest --config jest.config.cjs",
    "test:coverage": "npx jest --config jest.config.cjs --coverage",
    "test:e2e": "npx playwright test",
    "test:e2e:ui": "npx playwright test --ui",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "lint:backend": "eslint backend/**/*.js --max-warnings 0",
    "check:backend": "node scripts/check-backend-syntax.cjs",
    "verify-deployment": "node scripts/verify-deployment.js",
    "prepare": "node -e \"require('child_process').spawn('node', ['.husky/install.js'], {stdio: 'inherit'})\" || true"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@google/genai": "^1.30.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "framer-motion": "^12.0.0",
    "helmet": "^8.1.0",
    "howler": "^2.2.4",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.554.0",
    "multer": "^2.0.2",
    "music-metadata": "^11.10.2",
    "qrcode.react": "^4.2.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-easy-crop": "^5.5.6",
    "react-helmet-async": "^2.0.5",
    "react-qr-code": "^2.0.18",
    "react-router-dom": "^7.9.6",
    "recharts": "^3.7.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "sql.js": "^1.10.3",
    "ua-parser-js": "^2.0.8",
    "validator": "^13.15.23"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.0",
    "@playwright/test": "^1.58.0",
    "@types/howler": "^2.2.12",
    "@types/node": "^22.14.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.22",
    "concurrently": "^8.2.2",
    "jest": "^30.2.0",
    "nodemon": "^3.0.2",
    "postcss": "^8.5.6",
    "supertest": "^7.2.2",
    "tailwindcss": "^3.4.18",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  },
  "overrides": {
    "react-helmet-async": {
      "react": "$react"
    }
  }
}
